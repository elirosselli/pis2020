      "!**/node_modules/**",
      "!**/.prettierrc.js",
      "!**/babel.config.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "repository": {
    "type": "git",
