    "testWatch": "jest --watch --coverage=false --changedSince=origin/develop",
    "testDebug": "jest -o --watch --coverage=false",
    "linter": "eslint .",
    "linterFix": "eslint . --fix"
  },
  "jest": {
    "preset": "react-native",
    "collectCoverage": true,
