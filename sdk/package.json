    "eslint-plugin-sonarjs": "0.5.0",
    "jest": "26.6.0",
    "metro-react-native-babel-preset": "0.63.0",
    "react-test-renderer": "16.14.0"
  },
  "dependencies": {
    "async-mutex": "^0.2.4",
    "base-64": "0.1.0",
    "jsrsasign": "10.0.4",
    "mersenne-twister": "^1.1.0",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-native": "0.63.3",
    "react-native-ssl-pinning": "1.5.2"
  }
