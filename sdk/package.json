    "jest": "^26.4.2",
    "metro-react-native-babel-preset": "^0.63.0",
    "react-test-renderer": "^16.13.1"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "jsrsasign": "^10.0.4",
    "jsrsasign-util": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-native": "^0.63.2",
    "react-native-ssl-pinning": "^1.5.1"
  }
